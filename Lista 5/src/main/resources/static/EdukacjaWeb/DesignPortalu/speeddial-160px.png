                    <script>
                        jQuery( document ).ready( function() {
                            $ = jQuery;

							var main = $('#header');
							var menu = $('#header .main-menu ul .recruitment-8');
							var menuLi =  $('#header .main-menu ul li.more a');

							var mainRes = $('.touch #header');
							var menuRes = $('.touch #header .main-menu ul .recruitment-8');
							var menuLiRes =  $('.touch #header .main-menu ul li.more a');
							var clicked = false;

							var submenu = $('#menu-hover-8');
							var submenuRes = $('#menu-hover-4');

							var h1 = true, // mouseout from menu
								h2 = true; // mouseout from submenu
							if( !$('html').hasClass('touch') ){
								main.on('mousemove', function(e){
									if( e.type === 'mousemove' ) {
										if(!$(e.target).parent().hasClass('recruitment-8')){
											h1 = true;
											h2 = true;
											if(h1 == true && h2 == true) {
												submenu.hide();
												$('#header .main-menu ul li.more').removeClass('hovered');
											}
										}
									}
								});
								menuLi.on( 'mouseenter mouseleave', function(e){
									if( e.type === 'mouseenter' ){
										submenu.show();
										$('#header .main-menu ul li.more').addClass('hovered');
										h1 = false;
										h2 = false;
									} else if( e.type === 'mouseleave' ) {
										h1 = true;
										if(h1 == true && h2 == true) {
											submenu.hide();
										}
									}
								});
								submenu.on( 'mouseenter mouseleave', function(e){
									if( e.type === 'mouseenter' ){
										h2 = false;
										$('#header .main-menu ul li.more').addClass('hovered');
									} else if(e.type === 'mouseleave'){
										h2 = true;
										if(h1 == true && h2 == true) {
											submenu.hide();
											$('#header .main-menu ul li.more').removeClass('hovered');
										}
									}
								});
								$('#header .main-menu ul li.more').on( 'mouseover', function(){
									$('#header .main-menu ul li.more').addClass('hovered')
								});
								$('#header .main-menu ul li.more').on( 'mouseleave', function(){
									$('#header .main-menu ul li.more').removeClass('hovered')
								});
								$('#menu-hover-8 a').click(function(e) {
									submenu.hide();
								});
							}
							if( $('html').hasClass('touch') ){
								menuLiRes.click(function(e){
									if( clicked == false ){
										event.preventDefault();
										submenuRes.show();
										$('#header .main-menu ul li.more').addClass('hovered');
										clicked = true;
									} else if( clicked == true ) {
										submenuRes.hide();
										clicked = false;
									}
								});

								$('.touch body').click(function(e){
									if ( ( $(e.target).closest('#header .main-menu ul').length === 0 && $(e.target).closest('#submenu').length === 0 ) && $('#header .main-menu ul li.more').hasClass('hovered') ) {
										submenuRes.hide();
										clicked = false;
										return false;
									}
								});
							}

                        });

